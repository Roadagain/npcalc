{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended", "tslint-react", "tslint-config-airbnb"
    ],
    "jsRules": {},
    "rules": {
        "import-name": false,
        "variable-name": false,
        "function-name": false,
        "ter-indent": false,
        "indent": [true, "spaces", 4],
        "max-line-length": [
            true, 
            {
                "ignore-pattern": "^import |^export {(.*?)}"
            }
        ]
    },
    "rulesDirectory": []
}